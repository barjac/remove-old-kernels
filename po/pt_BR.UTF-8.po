msgid "Usage: <remove-old kernels|rok> [-a] [-A 0|1] [-c] [-t] [-f 0-9] [-F 0-9] [-n number] [-N number] [-p] [-q] [-Q 0|1][-l] [-m] [-v] [-h|-?]"
msgstr "Uso: <remove-old kernels|rok> [-a] [-A 0|1] [-c] [-t] [-f 0-9] [-F 0-9] [-n número] [-N número] [-p] [-q] [-Q 0|1][-l] [-m] [-v] [-h|-?]"

msgid "    -a = automatic, no questions. Interactive if not specified. (must be root)"
msgstr "    -a = automático, sem perguntas. Interativo se não especificado. (deve ser root)"

msgid "    -A value = 1 or 0 to turn ON or OFF automatic weekly removal of kernels. (e.g. -A1)"
msgstr "    -A valor = 1 ou 0 para ligar ou desligar a remoção semanal automática de kernels. (ex. -A1)"

msgid "    -c = as automatic but also checks for CRON=1 in the cfg file or exits. (must be root)"
msgstr "    -c = como automático, mas também verifica se CRON=1 no arquivo cfg ou sai. (deve ser root)"

msgid "    -t = Test mode, nothing is removed, urpme is simulated. Note: As root config may be updated"
msgstr "    -t = Modo de teste, nada é removido, urpme é simulado. Nota: Como root a configuração pode ser atualizada"

msgid "    -f value = number of the alternate .cfg file to use this time only. Exits if missing."
msgstr "    -f valor = número do arquivo .cfg alternativo para usar somente dessa vez. Sai se estiver faltando."

msgid "    -F value = number of alt .cfg file to use. This changes the ALTCFG= setting in the main .cfg file."
msgstr "    -F valor = número de arquivo .cfg alternativo a ser usado. Isso altera a configuração ALTCFG= no arquivo .cfg principal."

msgid "    -n value = number of kernels to keep this time only. (-n5 or -n 5 keep 5 kernels), Min=2, Default=3"
msgstr "    -n valor = número de kernels a manter apenas desta vez. (-n5 ou -n 5 mantém 5 kernels), Min=2, Padrão=3"

msgid "    -N value = number of kernels to keep. This changes the config file setting. (e.g. -N 5)"
msgstr "    -N valor = número de kernels a serem mantidos. Isso altera a configuração no arquivo. (ex. -N 5)"

msgid "    -p = preview the urpme commands which would be used."
msgstr "    -p = pré-visualizar os comandos urpme que seriam usados."

msgid "    -q = advanced mode, this time only - see man page."
msgstr "    -q = modo avançado, somente desta vez - veja a página de manual."

msgid "    -Q value = 1 or 0 (1 = ON) advanced mode, persistent. (e.g. -Q1)"
msgstr "    -Q valor = 1 ou 0 (1 = ligado) modo avançado, persistente. (ex. -Q1)"

msgid "    -l = list the last 1000 lines of the log."
msgstr "    -l = lista as últimas 1000 linhas do log."

msgid "    -m = mono. No colours in screen output."
msgstr "    -m = mono. Nenhuma cor na saída da tela."

msgid "    -v = version."
msgstr "    -v = versão."

msgid "    -? or -h = show this help."
msgstr "    -? ou -h = mostra essa ajuda."

msgid "KEY for column 3:"
msgstr "TECLA para coluna 3:"

msgid "     U = currently (U)sed running kernel."
msgstr "     U = kernel atualmente em (U)so."

msgid "     L = (L)IVE system original kernel - protected."
msgstr "     L = kernel original do sistema (L)IVE - protegido."

msgid "     V = keeping for (V)irtualbox. (only in advanced mode)"
msgstr "     V = mantido para (V)irtualbox. (somente no modo avançado)"

msgid "     X = keeping for (X)tables-addons (only in advanced mode)"
msgstr "     X = mantém para (X)tables-addons (somente no modo avançado)"

msgid "     K = keeping for installed (K)ernel. (only advanced mode)"
msgstr "     K = mantém para o (K)ernel instalado. (somente modo avançado)"

